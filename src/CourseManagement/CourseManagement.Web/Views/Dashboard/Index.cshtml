@model List<CourseEnrollMentVM>

    <div class="container-fluid">
        <div class="row">

            <!-- Main content -->
            <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                    <h1 class="h2">Course Enrollment Dashboard</h1>
                </div>

                <!-- Enrollment Statistics -->
                <div class="row mb-4">
                    <div class="col-md-4">
                        <div class="stat-box">
                            <h3>1,234</h3>
                            <p>Total Enrollments</p>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="stat-box">
                            <h3>56</h3>
                            <p>Active Courses</p>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="stat-box">
                            <h3>789</h3>
                            <p>Students</p>
                        </div>
                    </div>
                </div>

                <!-- Course Enrollment Table -->
                <div class="card dashboard-card mb-4">
                    <div class="dashboard-card-header">
                        Course Enrollment Details
                    </div>
                    <div class="dashboard-card-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th>Course Name</th>
                                        <th>Student Name</th>
                                        <th>Enrollment Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                 @foreach (var course in Model)
                                {
                                    <tr>
                                        <td>@course.CourseName</td>
                                        <td>@course.StudentName</td>
                                        <td>@course.EnrollmentDate</td>

                                    </tr>
                                }
                               
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>

                <!-- Additional Actions -->
                <div class="card dashboard-card">
                    <div class="dashboard-card-header">
                        Quick Actions
                    </div>
                    <div class="dashboard-card-body">
                        <button class="btn btn-success me-2">Add New Course</button>
                        <button class="btn btn-danger">Generate Report</button>
                    </div>
                </div>
            </main>
        </div>
    </div>


